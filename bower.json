{
  "name": "gulp-for-drupal",
  "version": "1.1.0",
  "versiondate": "2016-09-05",
  "dependencies": {
    "bootstrap": "v4.0.0-alpha.3",
    "normalize-css": "normalize.css#^4.2.0"
  },
  "install": {
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/js/dist/**",
        "bower_components/bootstrap/scss/**"
      ]
    },
    "base": "libs",
    "path": {
      "js": "js/{name}",
      "css": "css{name}",
      "/[sc|le]ss$/": "scss/{name}",
      "eot": "fonts/{name}",
      "svg": "fonts/{name}",
      "ttf": "fonts/{name}",
      "woff": "fonts/{name}",
      "map": "sourcemaps/{name}"
    }
  }
}
