{
  "name": "gulp-for-drupal",
  "version": "1.2.0",
  "versiondate": "2016-09-19",
  "dependencies": {
    "bootstrap": "v4.0.0-alpha.4",
    "normalize-css": "normalize.css#^4.2.0"
  },
  "install": {
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/js/dist/**",
        "bower_components/bootstrap/scss/**"
      ]
    },
    "base": "",
    "path": {
      "js": "js/libs/{name}",
      "css": "css/libs/{name}",
      "/[sc|le]ss$/": "scss/libs/{name}",
      "eot": "fonts/{name}",
      "svg": "fonts/{name}",
      "ttf": "fonts/{name}",
      "woff": "fonts/name}",
      "map": "sourcemaps/libs/{name}"
    }
  }
}
